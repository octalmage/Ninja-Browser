bs_package_flags = -bs-package-map ninja-browser
src_root_dir = /Users/jason.stallings/Documents/Apps/Ninja-Browser
bsc = /usr/local/lib/node_modules/bs-platform/bin/bsc.exe
bsdep = /usr/local/lib/node_modules/bs-platform/bin/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/Users/jason.stallings/Documents/Apps/Ninja-Browser/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
refmt = /usr/local/lib/node_modules/bs-platform/bin/refmt3.exe
reason_react_jsx = 
refmt_flags = --print binary
namespace = -ns NinjaBrowser
bsb_dir_group = 0
bsc_lib_includes = -I . -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/getLargestElement.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/getLargestElement.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/getLargestElement.mlast.d : build_deps src/getLargestElement.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/getLargestElement-NinjaBrowser.cmj |  $src_root_dir/src/getLargestElement.bs.js src/getLargestElement-NinjaBrowser.cmi : build_cmj_cmi src/getLargestElement.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build NinjaBrowser.cmi : build_package NinjaBrowser.mlmap
build build.ninja : phony  ||  NinjaBrowser.cmi src/getLargestElement.mlast.d
